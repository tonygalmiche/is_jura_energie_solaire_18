<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_company_form_jes" model="ir.ui.view">
        <field name="name">res.company.form.jes</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook" position="inside">
                <page string="JES" name="jes">
                    <group>
                        <field name="is_conditions_generales_ids" widget="many2many_binary"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>

</odoo>
